{
  "name": "typeform-builder",
  "version": "0.0.1",
  "description": "Builder",
  "main": "server.js",
  "scripts": {
    "test": "npm run lint;mocha --recursive --compilers js:babel/register",
    "test:watch": "npm test -- --watch --growl",
    "lint": "standard",
    "start": "NODE_ENV=prod node server.js",
    "dev": "npm run dev:web&npm run dev:mobile",
    "build": "npm run build:web;npm run build:mobile",
    "clean:web": "rm -rf dist-web",
    "clean:mobile": "rm -rf www",
    "dev:web": "NODE_ENV=dev node server-dev.js",
    "dev:mobile": "NODE_ENV=dev MOBILE=true PORT=3001 node server-dev.js",
    "build:web": "npm run clean:web; NODE_ENV=prod webpack",
    "build:mobile": "npm run clean:mobile; NODE_ENV=prod MOBILE=true webpack"
  },
  "license": "MIT",
  "dependencies": {
    "babel": "^5.8.12",
    "babel-core": "^5.6.18",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.1.4",
    "classnames": "^2.1.3",
    "codemirror": "^5.7.0",
    "eslint": "^1.5.1",
    "eslint-plugin-react": "^3.4.2",
    "exports-loader": "^0.6.2",
    "express": "^4.13.3",
    "file-loader": "0.8.4",
    "html-webpack-plugin": "1.6.2",
    "imports-loader": "^0.6.4",
    "lodash": "^3.10.1",
    "marked": "^0.3.5",
    "mocha": "^2.3.3",
    "raw-loader": "^0.5.1",
    "react": "^0.14.2",
    "react-codemirror": "^0.1.6",
    "react-dom": "^0.14.0",
    "react-hot-loader": "^1.2.7",
    "react-json-inspector": "^5.2.1",
    "react-redux": "^0.2.1",
    "redux": "^1.0.0-rc",
    "script-loader": "0.6.1",
    "standard": "^5.3.1",
    "style-loader": "^0.12.4",
    "webpack": "^1.9.11",
    "webpack-cordova-plugin": "0.1.5",
    "webpack-dev-server": "^1.9.0",
    "whatwg-fetch": "^0.10.0"
  },
  "engines": {
    "node": "4.1.1"
  },
  "devDependencies": {
    "glob": "^5.0.15"
  }
}
